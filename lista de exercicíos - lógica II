#include <stdio.h>
 #include <conio.h>
  #include <math.h>
  
  // Questão 04
    
 #define PI 3.14
  
 float volume(float raio){
     float v= 4/3 * (PI*pow(raio,3));
     retornar v;
  }
  
 int main(){
     float raio,resultado;
  
     printf(Escreva o raio de uma esfera para calcular o seu volume:");
     scanf("%f",&raio);
      
     resultado=volume(raio);
     printf("%.3f\n",resultado);
  
     getchar();
     retornar 0;
 }
