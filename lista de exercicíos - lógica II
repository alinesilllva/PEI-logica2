#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

int MDC(int x, int y){
    int menor, resultado = 1;

      if(x < y){
          menor = x;
    } else {
        menor = y;
    }

    for (int d  = 1;  d <= menor; d++){
        if(x % d == 0 and y % d == 0){
            resultado = d;
        }
    }
    return resultado;
}

