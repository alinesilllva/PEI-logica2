#include <studio.h>
 #include <stdlib.h>
  #include <locale.h>
  
  // Questão 08
  
int soma (int valor){
    int soma = 0
        for(int n = valor; n>=1; n--){
            soma = soma + n;
        }
        return soma;
}

int main (){
 setlocale(LC_ALL, "portuguese_Brazil");
 int n, resposta;

 print("digite o valor de n: ")
 scanf("%d, &n);

 resposta = soma(n);
 prinf("o resultado da soma vai ser: %d", resposta);
 
 return 0;

