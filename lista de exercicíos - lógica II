 #include <stdio.h>
  #include <locale.h>
   
  // Questão 19
   
 int fatorial(int n) {
     int resultado = 1;
     enquanto (n > 1) resultado *= n--;
     resultado de retorno;
 }
 int main(void) {
          
         setlocale(LC_ALL, "Português_Brasil");
          
     int n;
     printf("Escreva o numero a ser fatorado:\n");
     scanf("%d",&n);
     printf("%d", fatorial(n));
 }
 
 
 
